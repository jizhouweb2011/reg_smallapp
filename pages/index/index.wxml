<view class="page">
  <view class="weui-article__p">
  <image class="weui-article__img" src="../../img/ad.jpg" mode="scaleToFill" style="height: 120px;width:100%;" />
  </view>
  <form bindsubmit="formSubmit">
        <view class="weui-cells__title">单位名称</view>
        <view class="weui-cells weui-cells_after-title">
            <view class="weui-cell weui-cell_active">
                <view class="weui-cell__bd">
                    <input class="weui-input" placeholder="请输入公司或个体户名称" name="unit"/>
                </view>
            </view>
        </view>

        <view class="weui-cells__title">信用代码</view>
        <view class="weui-cells weui-cells_after-title">
            <view class="weui-cell weui-cell_active">
                <view class="weui-cell__bd">
                    <input class="weui-input" placeholder="请输入统一信用代码" name="code"/>
                </view>
            </view>
        </view>

        <view class="weui-cells__title">法人姓名</view>
        <view class="weui-cells weui-cells_after-title">
            <view class="weui-cell weui-cell_active">
                <view class="weui-cell__bd">
                    <input class="weui-input" placeholder="请输入法人姓名" name="name"/>
                </view>
            </view>
        </view>

        <view class="weui-cells__title">法人微信</view>
        <view class="weui-cells weui-cells_after-title">
            <view class="weui-cell weui-cell_active">
                <view class="weui-cell__bd">
                    <input class="weui-input" placeholder="请输入法人微信号" name="wx"/>
                </view>
            </view>
        </view>
<view class="weui-btn-area"><button class="weui-btn" type="primary" formType="submit">提交注册</button></view>
</form>

</view>
